{
  "expo": {
    "name": "TEMIS",
    "slug": "temis-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/TEMIS_Portada.png",
    "splash": {
      "image": "./assets/Temis_Fondo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
